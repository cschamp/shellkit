#############################################################################
#
#	Set where CodeWarrior is installed
#
#	If you install CodeWarrior somewhere other than its default location in
#	'/Applications' then you will have to modify the CWINSTALL variable to
#	reference the new location.
#
#############################################################################

setenv CWINSTALL /Applications/Metrowerks\ CodeWarrior\ 8.0/Metrowerks\ CodeWarrior

#############################################################################
#
#	Setup variables used by the CodeWarrior command line tools
#
#	You should not have to modify anything in this section.
#
#############################################################################

### Generic includes for all compilers:
### Paths are separated by colons (:) or commas (,)
# (This setup does not use a generic include for all compilers.)
# setenv MWCIncludes generic-paths-go-here

### Specific includes for individual compilers:
### Paths are separated by colons (:) or commas (,)
setenv MWCMacOSXPPCIncludes "$CWINSTALL"/MSL/MSL_C/MSL_Common/Include:"$CWINSTALL"/MSL/MSL_C/MSL_MacOS/Include:"$CWINSTALL"/MSL/MSL_C++/MSL_COMMON/Include:/usr/include:/usr/include/gcc/darwin/2.95.2
setenv MWCMachPPCIncludes /usr/include:"$CWINSTALL"/MSL/MSL_C/MSL_Common/Include:"$CWINSTALL"/MSL/MSL_C/MSL_MacOS/Include:"$CWINSTALL"/MSL/MSL_C++/MSL_COMMON/Include:/usr/include/gcc/darwin/2.95.2
setenv MWPEFCIncludes "$CWINSTALL"/MSL/MSL_C/MSL_Common/Include:"$CWINSTALL"/MSL/MSL_C/MSL_MacOS/Include:"$CWINSTALL"/MSL/MSL_C++/MSL_COMMON/Include:"$CWINSTALL"/MSL/MSL_Extras/MSL_Common/Include:"$CWINSTALL"/MSL/MSL_Extras/MSL_MacOS/Include:"$CWINSTALL"/MacOS\ Support/Universal/Interfaces/CIncludes

### Generic path to libraries for all linkers:
### Paths are separated by colons (:) or commas (,)
# (This setup does not use a generic library path for all compilers.)
# setenv MWLibraries generic-paths-go-here

### Specific path to libraries for individual linkers:
### Paths are separated by colons (:) or commas (,)
setenv MWMacOSXPPCLibraries "$CWINSTALL"/MacOS\ X\ Support/Libraries/Runtime/Libs:"$CWINSTALL"/MSL/MSL_C/MSL_MacOS/Lib/Mach-O:"$CWINSTALL"/MSL/MSL_C++/MSL_MacOS/Lib/Mach-O:/usr/lib
setenv MWMachPPCLibraries "$CWINSTALL"/MacOS\ X\ Support/Libraries/Runtime/Libs:"$CWINSTALL"/MSL/MSL_C++/MSL_MacOS/Lib/Mach-O:/usr/lib
setenv MWPEFLibraries "$CWINSTALL"/MacOS\ Support/Libraries/Runtime/Libs:"$CWINSTALL"/MSL/MSL_C/MSL_MacOS/Lib/PPC:"$CWINSTALL"/MSL/MSL_C++/MSL_MacOS/Lib/PPC:"$CWINSTALL"/MacOS\ Support/Universal/Libraries/StubLibraries

### Generic library files for all linkers:
### Paths are separated by colons (:) or commas (,)
# (This setup does not use a generic library list for all compilers.)
# setenv MWLibraryFiles generic-libraries-go-here

### Specific library files for individual linkers:
### Paths are separated by colons (:) or commas (,)
setenv MWMacOSXPPCLibraryFiles MSL_All_Mach-O.lib
setenv MWMachPPCLibraryFiles MSL_Runtime_Mach-O.a:MSL_C++_Mach-O.a
setenv MWPEFLibraryFiles MSL_All_Carbon.Lib:CarbonLib

### Generic framework items for all Mach-O linkers:
### Items are separated by colons (:) and specific versions are desginated by a comma (,) and a version
### For example:  System,B:Carbon:CoreServices,A:
setenv MWFrameworkPaths /System/Library/Frameworks
setenv MWFrameworkVersions System

#############################################################################
#
#	End of CodeWarrior setup
#
#############################################################################
